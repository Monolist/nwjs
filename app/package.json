    "min_width": 600,
    "min_height": 450,
    "single-instance": true
  },
  "dependencies": {
    "superagent": "^0.18.2",
    "humps": "0.0.1",
    "moment": "^2.8.3",
    "browserify": "^5.10.0",
    "browserify-shim": "^3.7.0",
    "react": "^0.12.2",
    "react-router": "^0.11.6",
    "reflux": "^0.2.3",
    "dancer": "git://github.com/jakemmarsh/dancer.js.git#master",
    "jquery": "^2.1.1",
    "when": "^3.6.0",
    "slug": "^0.8.0",
    "lodash": "^2.4.1",
    "bootstrap-tokenfield": "^0.12.0",
    "react-masonry-mixin": "git://github.com/jakemmarsh/react-masonry-mixin.git#master",
    "node-notifier": "^4.0.3"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "dancer": "./node_modules/dancer/dancer.js",
    "dancer-waveform": "./node_modules/dancer/plugins/dancer.waveform.js"
  },
  "browserify-shim": {
    "": ""
  }
