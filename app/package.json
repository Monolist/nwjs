{
  "name": "Monolist",
  "version": "0.1.0",
  "author": "Jake Marsh <jakemmarsh@gmail.com",
  "node-main": "./catchExceptions.js",
  "main": "./index.html",
  "webkit": {
    "page-cache": false
  },
  "window": {
    "title": "Monolist",
    "icon": "./icon.png",
    "toolbar": true,
    "frame": true,
    "width": 1225,
